<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hzr.cloudstation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/MyActivityTheme"
            android:windowSoftInputMode="stateHidden|adjustPan"/>
        <activity
            android:name=".SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:theme="@style/SplashTheme" />
        <activity
            android:name=".RegisterActivity"
            android:theme="@style/MyActivityTheme" />
        <activity
            android:name=".InfoSetActivity"
            android:theme="@style/SplashTheme" />
        <activity
            android:name=".AddressManageActivity"
            android:theme="@style/SplashTheme" />
        <activity
            android:name=".FeedBackActivity"
            android:theme="@style/SplashTheme" />
        <activity
            android:name=".ChangePwdActivity"
            android:theme="@style/SplashTheme" />
        <activity
            android:name=".InsertEmpActivity"
            android:theme="@style/SplashTheme" />
        <activity
            android:name=".EmpEditActivity"
            android:theme="@style/SplashTheme" />
        <activity
            android:name=".MyExpressActivity"
            android:theme="@style/SplashTheme" />
        <activity
            android:name=".ManageExpressActivity"
            android:theme="@style/SplashTheme" />
        <activity
            android:name=".AddExpressActivity"
            android:theme="@style/SplashTheme" />
        <activity
            android:name=".ExpressDetailActivity"
            android:theme="@style/SplashTheme" />
        <activity android:name=".ChoseEmpActivity"
            android:theme="@style/SplashTheme"></activity>
    </application>

</manifest>